{
    "name": "discord-bot",
    "version": "0.1.0",
    "main": "src/start.ts",
    "scripts": {
        "start": "node --loader ts-node/esm/transpile-only src/start.ts",
        "dev": "nodemon --experimental-specifier-resolution=node --exec node --no-warnings=ExperimentalWarning --loader ts-node/esm/transpile-only src/start.ts",
        "build": "tsc",
        "serve": "node build/start.js"
    },
    "devDependencies": {
        "@types/node": "^18.7.13",
        "nodemon": "^2.0.19",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.2"
    },
    "dependencies": {
        "@discordx/importer": "^1.1.10",
        "discord.js": "^14.3.0",
        "discordx": "^11.1.9",
        "dotenv": "^16.0.1",
        "reflect-metadata": "^0.1.13"
    }
}
